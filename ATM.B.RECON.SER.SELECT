    EXECUTE 'CLEAR.FILE ' : FN.FLAT.FILE

    SEL.CMD = "SELECT ":FN.ATM.BRANCH
    CALL EB.READLIST(SEL.CMD,SEL.LIST,'',SEL.CNT,SEL.ERR)
