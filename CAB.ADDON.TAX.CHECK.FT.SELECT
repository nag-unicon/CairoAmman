    SUBROUTINE CAB.ADDON.TAX.CHECK.FT.SELECT

    $INSERT T24.BP I_COMMON
    $INSERT T24.BP I_EQUATE
    $INSERT CAB.BP I_CAB.ADDON.TAX.CHECK.FT

    Y.SEL.CMD = '' ; Y.SEL.LIST = ''
    Y.SEL.CMD = 'SELECT ':FN.TAX.FT.DETS:' LIKE ...-':Y.END.DATE:'... AND WITH FT.POSTED LIKE ...ERROR...'
    CALL EB.READLIST(Y.SEL.CMD,Y.SEL.LIST,'','','')

    IF Y.SEL.LIST NE '' THEN
        CALL BATCH.BUILD.LIST("",Y.SEL.LIST)
    END

    RETURN

END
