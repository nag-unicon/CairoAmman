    $INSERT T24.BP I_BATCH.FILES
    $INSERT CAB.BP I_CAB.AA.DELETE.SIM.ACTIVITIES.COMMON

    SEL.LIST = ""
    SEL.CNT  = ""
    SEL.ERR  = ""

    SEL.CMD = "SELECT ":FN.AA.ACTIVITY.HISTORY.SIM :" WITH ACTIVITY EQ LENDING-NEW-ARRANGEMENT"

