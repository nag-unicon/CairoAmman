    SUBROUTINE CAB.AA.ARR.UNAUTH.TO.LIVE.SELECT

    $INSERT I_COMMON
    $INSERT I_EQUATE

    OPEN '&SAVEDLISTS&' TO TMP.SAVE ELSE NULL

    SaveListName = "UNAUTH.TO.LIVE":'.':TODAY

    READ RREC FROM TMP.SAVE, SaveListName ELSE NULL

    IF RREC THEN
        SEL.LIST = RREC
    END ELSE
        SEL.CMD = "SELECT FBNK.AA.ARRANGEMENT.DATED.XREF"
        CALL EB.READLIST(SEL.CMD,SEL.LIST,'',SEL.CNT,SEL.ERR)
    END

    CALL BATCH.BUILD.LIST("",SEL.LIST)

    RETURN

END
