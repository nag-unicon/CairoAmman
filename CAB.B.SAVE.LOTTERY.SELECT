    SUBROUTINE CAB.B.SAVE.LOTTERY.SELECT

    $INSERT T24.BP I_COMMON
    $INSERT T24.BP I_EQUATE
    $INSERT CAB.BP I_CAB.LOT.COMMON

    EXECUTE 'CLEAR.FILE ': FN.FLAT.FILE

    SEL.CMD = 'SELECT ':FN.ACC: ' WITH CATEGORY EQ 6001 AND (CURRENCY EQ JOD OR CURRENCY EQ USD)'
    CALL EB.READLIST(SEL.CMD,SEL.LIST,'',SEL.CNT,SEL.ERR)
    LIST.PARAM = ''

    CALL BATCH.BUILD.LIST(LIST.PARAM,SEL.LIST)

    RETURN
END
