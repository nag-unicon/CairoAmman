    SUBROUTINE AA.LOAN.NOT.SAME.CUSTOMER.TXN.SELECT

    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT TT.BP I_AA.LOAN.NOT.SAME.CUSTOMER.TXN.COMMON

    EXECUTE 'CLEAR.FILE ' :FN.AA.DIFF.CUSTOMER.TXN

    SEL.CMD = "SELECT ":FN.AA.ARRANGEMENT :" WITH PRODUCT.LINE EQ LENDING AND (ARR.STATUS NE AUTH OR ARR.STATUS NE UNAUTH)"
    CALL EB.READLIST(SEL.CMD,SEL.LIST,'',SEL.CNT,SEL.ERR)

    CALL BATCH.BUILD.LIST("",SEL.LIST)

    RETURN

END

