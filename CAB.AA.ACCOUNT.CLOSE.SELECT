    SUBROUTINE CAB.AA.ACCOUNT.CLOSE.SELECT

    $INSERT I_COMMON
    $INSERT I_EQUATE

    OPEN '&SAVEDLISTS&' TO TMP.SAVE ELSE NULL
    READ SEL.LIST FROM TMP.SAVE, 'AA.ACCOUNT.CLOSE.LIST' ELSE NULL

    SEL.CMD  = "SELECT FBNK.ACCOUNT WITH ARRANGEMENT.ID NE ''"

    CALL EB.READLIST(SEL.CMD,SEL.LIST,'',SEL.CNT,SEL.ERR)

    CALL BATCH.BUILD.LIST("",SEL.LIST)

    RETURN

END
