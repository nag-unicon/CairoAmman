    SUBROUTINE BASE.II.PROCESS.SELECT
    $INCLUDE T24.BP I_COMMON
    $INCLUDE T24.BP I_EQUATE
    $INCLUDE CAB.BP I_BASE.II.PROCESS.COMMON

    FN.BASE.II.CONCAT = 'F.BASE.II.CONCAT'
    F.BASE.II.CONCAT = ''
    CALL OPF(FN.BASE.II.CONCAT,F.BASE.II.CONCAT)

    SEL.CMD = 'SELECT ':FN.BASE.II.CONCAT
    CALL EB.READLIST(SEL.CMD,SEL.LIST,'',NO.SEL,SEL.ERR)
    CALL BATCH.BUILD.LIST('',SEL.LIST)

    RETURN
END
