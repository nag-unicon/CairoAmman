    SUBROUTINE CAB.AI.FCY.COM(T1,T2,T3,T4,T5,T6,T7,T8,T9,CHG.AMT)
    $INCLUDE T24.BP I_COMMON
    $INCLUDE T24.BP I_EQUATE
    $INCLUDE T24.BP I_F.TELLER

    DR.AMT = ''
    DR.AMT.1 = ''
    CHG.AMT = ''
    YR.CHARGE = ''

    DR.CCY = R.NEW(TT.TE.CURRENCY.2)

    IF  DR.CCY EQ LCCY THEN
        RETURN
    END

    DR.AMT = R.NEW(TT.TE.AMOUNT.LOCAL.1)

    DR.AMT.1 = DR.AMT

    YR.CHARGE = DR.AMT.1 * 0.005

    CHG.AMT = YR.CHARGE


    RETURN
